{
  "manifest_version": 3,
  "name": "custom new tab",
  "version": "1.0",
  "description": "A new tab page which can be completely customizible",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "bookmarks",
    "favicon",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "focus-search": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Open the search bar."
    }
  }
}